    <link rel="stylesheet" type="text/css" href="{{ url_for("plugin_res", {"plugin": module_id, "path": "maps-legend.bundle.min.css"}) }}"/>
    <link rel="stylesheet" type="text/css" href="{{ url_for("plugin_res", {"plugin": module_id, "path": "galette_maps.css"}) }}"/>
{% endif %}
{% if cur_route == 'maps_localize_member' or cur_route == 'maps_mymap' %}
